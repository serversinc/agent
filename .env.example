PORT=7567

# URL of core service to send events to (required)
CORE_URL=

# Shared secret used to authenticate requests to the agent (required)
# Generate a secure random value (32+ bytes hex) and paste here
SECRET_KEY=

# Docker socket path (default: /var/run/docker.sock)
DOCKER_SOCKET=/var/run/docker.sock

# HTTP client timeout in ms
HTTP_TIMEOUT=5000

# Logger configuration
# Level: trace|debug|info|warn|error
LOGGER_LEVEL=info
# Pretty-print log output (true/false)
LOGGER_PRETTY=false
